<!--pages/login/login.wxml-->

<view >
    <view>请输入用户ID：</view>
    <input type="text" placeholder="请输入用户ID" bindinput="inputId"/>
    
    <view>请输入密码：</view>
    <input type="password" placeholder="请输入密码" bindinput="inputPassword"/>
    <button bindtap="submitForm">提交</button>
    <button bindtap="redirectToNoTabBar">注册</button>
</view>

<view>
  <button bindtap="redirectToTabBar">跳转到目标页面</button>
</view>

